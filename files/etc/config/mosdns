
config mosdns 'config'
	option geo_update_week_time '*'
	option geo_update_day_time '2'
	option enabled '0'
	option redirect '0'
	option configfile '/etc/mosdns/config_custom.yaml'
	list geoip_tags 'cn'
	option geo_auto_update '0'
	list geosite_tags 'cn'
	list geosite_tags 'apple-cn'
	list geosite_tags 'category-games@cn'
	list geosite_tags 'geolocation-!cn'
	list geosite_tags 'steam@cn'
	list geosite_tags 'microsoft-dev'
	list geosite_tags 'msn'
	list geosite_tags 'category-public-tracker'
	list geosite_tags 'microsoft'
	list geosite_tags 'category-dev'
	list geosite_tags 'google@cn'

